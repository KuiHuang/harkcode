import sys
import urllib2

if __name__ == '__main__':
    stdout_write = sys.stdout.write

    url = 'http://docs.python.org/library/urllib2.html'
    num_lines_to_read = 10

    resp = urllib2.urlopen(url)
    num_lines_to_read = 10
    for line in resp:
        if num_lines_to_read == 0:
            break
        stdout_write(line)
        num_lines_to_read -= 1

    resp.close()
